{{#if og}}
	<meta property="og:title" content="{{og.title}}"/>
	<meta property="og:image" content="https://bizinfluence.bizstore.io{{og.image}}"/>
	<meta property="og:image:type" content="{{og.imageType}}"/>
	<meta property="og:image:alt" content="https://bizinfluence.bizstore.io/img/default_campaign.png"/>
	<meta property="og:description" content="{{og.description}}"/>
	<meta property="og:url" content="https://bizinfluence.bizstore.io/{{og.url}}"/>
{{else}}
	<meta property="og:title" content="BizInfluence"/>
	<meta property="og:image" content="https://bizinfluence.bizstore.io/img/default_campaign.png"/>
	<meta property="og:description" content="Share & Earn Rewards"/>
	<meta property="og:url" content="https://bizinfluence.bizstore.io/"/>
{{/if}}